   ],
   "source": [
    "n = 10\n",
    "\n",
    "for i in range(n):\n",
    "    el = 1\n",
    "    for j in range(i+1):\n",
    "        print(el, end=' ')\n",
    "        el = el * (i-j) // (j+1)\n",
    "    print()"
   ]
